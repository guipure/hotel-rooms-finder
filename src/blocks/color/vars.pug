block fakeBlock
  - let squareClassName = 'color__square color__square_'
  - let number
  case theme
    when 'Dark Shade 100%'
      -
        number = '#1F2041'
        squareClassName += 'dark-shade-100'
    when 'Dark Shade 75%'
      -
        number = '#1F2041'
        squareClassName += 'dark-shade-75'
    when 'Dark Shade 50%'
      -
        number = '#1F2041'
        squareClassName += 'dark-shade-50'
    when 'Dark Shade 25%'
      -
        number = '#1F2041'
        squareClassName += 'dark-shade-25'
    when 'Dark Shade 5%'
      -
        number = '#1F2041'
        squareClassName += 'dark-shade-5'
    when 'Purple'
      -
        number = '#BC9CFF'
        squareClassName += 'purple'
    when 'Green'
      -
        number = '#6FCF97'
        squareClassName += 'green'
